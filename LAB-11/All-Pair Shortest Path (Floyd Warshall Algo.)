#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n = 4;
    int arr[4][4] = {
        {0, 5, 9999, 10},
        {9999, 0, 3, 9999},
        {9999, 9999, 0, 1},
        {9999, 9999, 9999, 0}
    };

    vector<vector<int> > dist(n, vector<int>(n));
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            dist[i][j] = arr[i][j];

    for (int k = 0; k < n; k++)
        for (int i = 0; i < n; i++)
            for (int j = 0; j < n; j++)
                if (dist[i][k] + dist[k][j] < dist[i][j])
                    dist[i][j] = dist[i][k] + dist[k][j];

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++)
            cout << dist[i][j] << " ";
        cout << endl;
    }
}
