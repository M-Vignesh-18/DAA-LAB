#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n = 4, W = 7;
    int wt[] = {1, 3, 4, 5};
    int val[] = {1, 4, 5, 7};
    vector<vector<int> > dp(n + 1, vector<int>(W + 1, 0));
    for (int i = 1; i <= n; i++)
        for (int w = 1; w <= W; w++)
            if (wt[i - 1] <= w)
                dp[i][w] = max(val[i - 1] + dp[i - 1][w - wt[i - 1]], dp[i - 1][w]);
            else
                dp[i][w] = dp[i - 1][w];
    cout << dp[n][W];
}
